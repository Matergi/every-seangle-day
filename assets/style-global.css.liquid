  }/* start scroll bar */:root
  {
    --scrollbar-size: 0.375rem;
    --scrollbar-minlength: 1.5rem; /* Minimum length of scrollbar thumb (width of horizontal, height of vertical) */
    --scrollbar-ff-width: thin; /* FF-only accepts auto, thin, none */
    --scrollbar-track-color: transparent;
    --scrollbar-color: rgba(0, 0, 0, 0.0);
    --scrollbar-color-hover-div: rgba(0, 0, 0, 0.2);
    --scrollbar-color-hover: rgba(0, 0, 0, 0.3);
    --scrollbar-color-active: rgb(0, 0, 0);
  }
  .layout-scrollbar::-webkit-scrollbar {
    height: var(--scrollbar-size);
    width: var(--scrollbar-size);
  }
  .scroll-horizontal {
    overscroll-behavior: contain;
    overflow-x: auto;
    -webkit-overflow-scrolling: touch;
    -ms-overflow-style: -ms-autohiding-scrollbar;
    scrollbar-width: var(--scrollbar-ff-width);
  }

  .scroll-vertical {
    overscroll-behavior: contain;
    overflow-y: auto;
    -webkit-overflow-scrolling: touch;
    -ms-overflow-style: -ms-autohiding-scrollbar;
    scrollbar-width: var(--scrollbar-ff-width);
    padding-right: 10px;
  }

  /* This class controls what elements have the new fancy scrollbar CSS */
  .layout-scrollbar {
    scrollbar-color: var(--scrollbar-color) var(--scrollbar-track-color);
  }

  .layout-scrollbar:hover {
    scrollbar-color: var(--scrollbar-color-hover-div) var(--scrollbar-track-color);
  }

  .layout-scrollbar::-webkit-scrollbar-track {
    background-color: var(--scrollbar-track-color);
  }
  .layout-scrollbar::-webkit-scrollbar-thumb {
    background-color: var(--scrollbar-color);
    border-radius: 3px;
  }
  .layout-scrollbar:hover::-webkit-scrollbar-thumb {
    background-color: var(--scrollbar-color-hover-div);
    border-radius: 3px;
  }
  .layout-scrollbar::-webkit-scrollbar-thumb:hover {
    background-color: var(--scrollbar-color-hover);
  }
  .layout-scrollbar::-webkit-scrollbar-thumb:active {
    background-color: var(--scrollbar-color-active);
  }
  .scrollbar-test::-webkit-scrollbar-thumb:vertical,
  .layout-scrollbar::-webkit-scrollbar-thumb:vertical {
    min-height: var(--scrollbar-minlength);
  }
  .scrollbar-test::-webkit-scrollbar-thumb:horizontal,
  .layout-scrollbar::-webkit-scrollbar-thumb:horizontal {
    min-width: var(--scrollbar-minlength);
  }
  /* end scroll bar */

  /* start modal */
  .custom-modal-wrap {
    position: fixed;
    inset: 0;
    display: flex;
    -webkit-box-align: center;
    align-items: center;
    -webkit-box-pack: center;
    justify-content: center;
    z-index: 1001;
    background-color: rgba(0, 0, 0, 0.5);
  }
  .custom-modal {
    position: relative;
    animation-name: transition-in;
    animation-duration: 650ms;
    animation-direction: normal;
    animation-iteration-count: 1;
    animation-timing-function: ease-out;
    animation-fill-mode: both;
    max-height: 95%;
    max-width: 100%;
  }
  .modal-close-icon {
    position: relative;
    margin: 10px;
    text-indent: -9999em;
    background-color: transparent;
    transition: transform 250ms ease-in-out 0s;
  }
  .modal-close-icon::before,
  .modal-close-icon::after {
    background-color: rgb(255, 255, 255);
    content: "";
    position: absolute;
    width: 2px;
    top: 50%;
    left: 50%;
    background-color: rgb(33, 42, 47);
    transition: transform 250ms ease-in-out 0s;
  }
  @media only screen and (max-width: 777px) {
    .custom-modal {
      width: 100vw;
    }
    .modal-close-icon {
      height: 30px;
      width: 30px;
    }
    .modal-close-icon::before,
    .modal-close-icon::after {
      height: 30px;
    }
  }
  @media only screen and (min-width: 777px) and(max-width: 990px) {
    .custom-modal {
      width: 80vw;
    }
    .modal-close-icon {
      height: 40px;
      width: 40px;
    }
    .modal-close-icon::before,
    .modal-close-icon::after {
      height: 40px;
    }
  }
  @media only screen and (min-width: 990px) {
    .custom-modal {
      width: 65vw;
    }
    .modal-close-icon {
      height: 40px;
      width: 40px;
    }
    .modal-close-icon::before,
    .modal-close-icon::after {
      height: 40px;
    }
  }
  .modal-close {
    position: absolute;
    top: 0;
    right: 0;
    z-index: 1;
  }
  .modal-close > div {
    display: flex;
    -webkit-box-pack: center;
    justify-content: center;
    -webkit-box-align: center;
    align-items: center;
    min-width: 42px;
    min-height: 42px;
    outline: 0;
    background-color: transparent;
    border: 0;
    cursor: pointer;
    padding: 0;
    appearance: none;
  }
  .modal-close-icon:hover {
    transform: rotate(90deg);
  }
  .modal-close-icon.dark::before,
  .modal-close-icon.dark::after {
    background-color: rgb(0, 0, 0);
  }
  .modal-close-icon.light::before,
  .modal-close-icon.light::after {
    background-color: rgb(255, 255, 255);
  }
  .modal-close-icon::before {
    transform: translate(-50%, -50%) rotate(45deg);
  }
  .modal-close-icon::after {
    transform: translate(-50%, -50%) rotate(-45deg);
  }
  /* end modal */

